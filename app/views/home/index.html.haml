      %p
        %ol
          %li
            = link_to 'Register', signup_path
            an account
          %li
            = link_to 'Upload', new_tag_path
            tags
    %br.clear     

  #tags
    - if @tags.blank?
      %h2.error No tags yet!
    - else
      %ul
        - @tags.each do |tag|
          - user = tag.user
          %li.floatLeft= link_to image_tag(tag.image.url(:small)), (user.nil? ? tag_path(tag) : user_tag_path(user, tag))
    %br.clear
